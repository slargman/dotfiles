[user]
	name = Shannon Largman
	email = shannon.largman@gmail.com
[color]
	ui = auto
[core]
	editor = nvim
	autocrlf = input
	safecrlf = true
	excludesfile = /Users/shannon/.gitignore_global
[alias]
  co = checkout
  ci = commit
  st = status
  br = branch
  hist = log --pretty=format:'%h %ad | %s%d [%an]' --graph --date=short
  type = cat-file -t
  dump = cat-file -p
[init]
	defaultBranch = main
[pull]
	ff = only
